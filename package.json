    "sweetalert2": "^9.17.1",
    "v-avatar": "^0.1.1",
    "v-charts": "^1.19.0",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-animate": "^0.0.4",
    "vue-axios": "^3.2.4",
    "vue-chartist": "^2.2.1",
    "vue-i18n": "^8.14.1",
    "vue-marquee-text-component": "^1.1.1",
    "vue-moment": "^4.1.0",
    "vue-number-animation": "^1.0.5",
    "vue-quill": "^1.5.1",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.3",
    "vue-signature-pad": "^2.0.0",
    "vue-sweetalert2": "^4.3.1",
    "vue-text-editor": "^1.1.1",
